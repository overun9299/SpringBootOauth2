  <select id="selectPermissionByUserId" resultMap="BaseResultMapPer">
    SELECT
      p.*
    FROM
      ac_user AS u
      LEFT JOIN ac_user_role AS ur
        ON u.id = ur.user_id
      LEFT JOIN ac_role AS r
        ON r.id = ur.role_id
      LEFT JOIN ac_role_permission AS rp
        ON r.id = rp.role_id
      LEFT JOIN ac_permission AS p
        ON p.id = rp.permission_id
    WHERE u.id = ${userId}
  </select>
